{
  "git": {
    "deploymentEnabled": {
      "master": false
    }
  },
  "env": {
    "PNPM_HOME": "/pnpm",
    "NPM_CONFIG_USERCONFIG": "/pnpm/.npmrc"
  },
  "build": {
    "env": {
      "PNPM_VERSION": "latest"
    },
    "packageManager": "pnpm",
    "installCommand": "pnpm install --frozen-lockfile",
    "buildCommand": "pnpm turbo run build --filter=web"
  },
  "outputDirectory": "apps/web/.next"
}
